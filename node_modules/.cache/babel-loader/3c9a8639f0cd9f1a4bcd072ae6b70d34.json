{"ast":null,"code":"import { Dashboard } from \"@uppy/vue\";\nimport \"@uppy/core/dist/style.css\";\nimport Uppy from \"@uppy/core\";\nimport XHRUpload from \"@uppy/xhr-upload\";\nimport \"@uppy/core/dist/style.css\";\nimport \"@uppy/dashboard/dist/style.css\";\nimport App from \"../App.vue\";\nexport default {\n  name: 'Hello',\n  components: {\n    Dashboard,\n    App\n  },\n  data: () => {\n    return {\n      IP: '',\n      why: '',\n      reason: '',\n      dep: '',\n      email: '',\n      mac1: '',\n      mac2: '',\n      num: '',\n      peo: '',\n      picked: '임직원',\n      elec: '노트북',\n      radioValue1: '임직원',\n      radioValue2: '외부인력',\n      radioValue3: '노트북',\n      radioValue4: '데스크탑',\n      radioValue5: '기타'\n    };\n  },\n  methods: {\n    upload() {\n      alert(\"IP주소 : \" + this.IP + \"\\n\" + \"담당자 : \" + this.peo);\n      alert(\"유선MAC : \" + this.mac1);\n      alert(\"무선MAC : \" + this.mac2);\n      alert(\"구분 : \" + this.picked);\n      alert(\"부서 : \" + this.dep);\n      alert(\"ID : \" + this.email);\n      alert(\"용도 : \" + this.why);\n      alert(\"단말기 : \" + this.elec);\n      alert(\"사유 : \" + this.reason);\n      alert(\"비고 : \" + this.num);\n    }\n  },\n  computed: {\n    uppy() {\n      return new Uppy().use(XHRUpload, {\n        endpoint: \"http://10.1.30.202:5000/upload\",\n        fieldName: \"file\",\n        bundle: true,\n        autoProceed: false\n      }).on('upload-success', (file, response) => {\n        console.log(file.name, response);\n        const users = response.body;\n        this.dep = users.dep;\n        this.email = users.email;\n        this.mac1 = users.mac1;\n        this.mac2 = users.mac2;\n        this.num = users.num;\n        this.peo = users.peo;\n      });\n    },\n    beforeDestroy() {\n      this.uppy.close({\n        reason: 'unmount'\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAiCA,SAASA,SAAQ,QAAS,WAAW;AACrC,OAAO,2BAA2B;AAClC,OAAOC,IAAG,MAAO,YAAY;AAC7B,OAAOC,SAAQ,MAAO,kBAAkB;AACx