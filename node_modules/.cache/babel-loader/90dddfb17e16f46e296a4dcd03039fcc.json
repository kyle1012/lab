{"ast":null,"code":"import { Dashboard } from \"@uppy/vue\";\nimport \"@uppy/core/dist/style.css\";\nimport Uppy from \"@uppy/core\";\nimport XHRUpload from \"@uppy/xhr-upload\";\nimport \"@uppy/core/dist/style.css\";\nimport \"@uppy/dashboard/dist/style.css\";\nimport App from \"../App.vue\";\nexport default {\n  name: 'Hello',\n  components: {\n    Dashboard,\n    App\n  },\n  data: () => {\n    return {\n      dep: '',\n      email: '',\n      mac1: '',\n      mac2: '',\n      num: '',\n      peo: '',\n      picked: '',\n      checked: '',\n      radioValue1: '임직원',\n      radioValue2: '외부인력',\n      radioValue3: '노트북',\n      radioValue4: '데스크탑',\n      radioValue5: '기타'\n    };\n  },\n  computed: {\n    uppy() {\n      return new Uppy().use(XHRUpload, {\n        endpoint: \"http://10.1.30.202:5000/upload\",\n        fieldName: \"file\",\n        bundle: true,\n        autoProceed: false\n      }).on('upload-success', (file, response) => {\n        console.log(file.name, response);\n        const users = response.body;\n        this.dep = users.dep;\n        this.email = users.email;\n        this.mac1 = users.mac1;\n        this.mac2 = users.mac2;\n        this.num = users.num;\n        this.peo = users.peo;\n      });\n    },\n    beforeDestroy() {\n      this.uppy.close({\n        reason: 'unmount'\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA8BA,SAASA,SAAQ,QAAS,WAAW;AACrC,OAAO,2BAA2B;AAClC,OAAOC,IAAG,MAAO,YAAY;AAC7B,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAO,2BAA2B;AAClC,OAAO,gCAAgC;AACvC,OAAOC,GAAE,MAAO,YAAW;AAE3B,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IACVL,SAAS;IACTG;EACF,CAAC;EACDG,IAAI,EAAE,MAAM;IACV,OAAO;MACLC,GAAG,EAAE,EAAE;MACPC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAC,EAAE;MACPC,IAAI,EAAC,EAAE;MACPC,GAAG,EAAC,EAAE;MACNC,GAAG,EAAC,EAAE;MACNC,MAAM,EAAC,EAAE;MACTC,OAAO,EAAC,EAAE;MACVC,WAAW,EAAC,KAAK;MACjBC,WAAW,EAAC,MAAM;MAClBC,WAAW,EAAC,KAAK;MACjBC,WAAW,EAAC,MAAM;MAClBC,WAAW,EAAC;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,IAAI,GAAE;MACJ,OAAO,IAAIpB,IAA