{"ast":null,"code":"import { Dashboard } from '@uppy/vue';\nimport '@uppy/core/dist/style.css';\nimport '@uppy/dashboard/dist/style.css';\nimport Uppy from '@uppy/core';\nimport Webcam from '@uppy/webcam';\nexport default {\n  name: 'App',\n  components: {\n    Dashboard\n  },\n  computed: {\n    uppy: () => new Uppy().use(Webcam)\n  },\n  beforeDestroy() {\n    this.uppy.close({\n      reason: 'unmount'\n    });\n  }\n};","map":{"version":3,"mappings":"AAOA,SAASA,SAAQ,QAAS,WAAU;AAEpC,OAAO,2BAA0B;AACjC,OAAO,gCAA+B;AAEtC,OAAOC,IAAG,MAAO,YAAW;AAC5B,OAAOC,MAAK,MAAO,cAAa;AAEhC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,QAAQ,EAAE;IACRC,IAAI,EAAE,MAAM,IAAIL,IAAI,EAAE,CAACM,GAAG,CAACL,MAAM;EACnC,CAAC;EACDM,aAAY,GAAK;IACf,IAAI,CAACF,IAAI,CAACG,KAAK,CAAC;MAAEC,MAAM,EAAE;IAAU,CAAC;EACvC;AACF","names":["Dashboard","Uppy","Webcam","name","components","computed","uppy","use","beforeDestroy","close","reason"],"sourceRoot":"","sources":["/lab/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <dashboard :uppy=\"uppy\" :plugins=\"['Webcam']\" :props=\"{theme: 'light'}\" />\n  </div>\n</template>\n\n<script>\nimport { Dashboard } from '@uppy/vue'\n\nimport '@uppy/core/dist/style.css'\nimport '@uppy/dashboard/dist/style.css'\n\nimport Uppy from '@uppy/core'\nimport Webcam from '@uppy/webcam'\n\nexport default {\n  name: 'App',\n  components: {\n    Dashboard\n  },\n  computed: {\n    uppy: () => new Uppy().use(Webcam)\n  },\n  beforeDestroy () {\n    this.uppy.close({ reason: 'unmount' })\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}