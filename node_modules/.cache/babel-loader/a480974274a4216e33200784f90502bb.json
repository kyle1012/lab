onsole.log(response);\n        alert(response.data);\n        self.$router.push('home');\n      }).catch(function (error) {\n        console.log(error);\n        alert(error.response.data);\n      });\n    }\n  },\n  computed: {\n    uppy() {\n      return new Uppy().use(XHRUpload, {\n        endpoint: \"http://10.1.30.202:5000/int_upload\",\n        fieldName: \"file\",\n        bundle: true,\n        autoProceed: false\n      }).on('upload-success', (file, response) => {\n        console.log(file.name, response);\n        const int_doc = response.body;\n        this.dep = int_doc.int_dep;\n        this.peo = int_doc.int_peo;\n        this.IP = int_doc.int_ip;\n        this.period = int_doc.int_period;\n        this.site = int_doc.int_site;\n        this.whyy = int_doc.int_why;\n        this.doc = int_doc.int_doc;\n        this.time = int_doc.int_time;\n        this.title = int_doc.int_title;\n        this.file_path = int_doc.int_file_path;\n        this.file_name = int_doc.file_name;\n        this.pdf_file = '/INTERNET_Doc/' + this.file_name;\n      });\n    }\n  },\n  beforeDestroy() {\n    this.uppy.close({\n      reason: 'unmount'\n    });\n  }\n};","map":{"version":3,"mappings":";AAwEE,SAASA,SAAQ,QAAS,WAAW;AACrC,OAAO,2BAA2B;AAClC,OAAOC,IAAG,MAAO,YAAY;AAC7B,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAO,2BAA2B;AAClC,OAAO,gCAAgC;AACvC,OAAOC,GAAE,MAAO,YAAW;AAC3B,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,YAAW;AAG5B,eAAc;EACZC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IACVP,SAAS;IACTG,GAAG;IACHE;EACF,CAAC;EAEDG,IAAI,EAAE,MAAM;IACV,OAAO;MACLC,EAAC,EAAI,EAAE;MACPC,GAAE,EAAI,EAAE;MACRC,GAAG,EAAE,EAAE;MACPC,GAAG,EAAC,EAAE;MACNC,GAAG,EAAC,EAAE;MACNC,IAAI,EAAC,EAAE;MACPC,MAAM,EAAC,EAAE;MACTC,IAAI,EAAC,EAAE;MACPC,IAAI,EAAC,IAAI;MACTC,KAAK,EAAC,IAAI;MACVC,WAAW,EAAC,IAAI;MAChBC,WAAW,EAAC,IAAI;MAChBC,WAAW,EAAC,IAAI;MAChBC,IAAI,EAAC,EAAE;MACPC,KAAK,EAAC,EAAE;MACRC,SAAS,EAAC,EAAE;MACZC,QAAQ,EAAC,EAAE;MACXC,SAAS,EAAC,EAAE;MACZC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAM,GAAE;MACR,IAAIC,