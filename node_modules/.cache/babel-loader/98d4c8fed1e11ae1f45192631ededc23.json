{"ast":null,"code":"import { Dashboard } from \"@uppy/vue\";\nimport \"@uppy/core/dist/style.css\";\nimport Uppy from \"@uppy/core\";\nimport XHRUpload from \"@uppy/xhr-upload\";\nimport \"@uppy/core/dist/style.css\";\nimport \"@uppy/dashboard/dist/style.css\";\nexport default {\n  name: 'App',\n  components: {\n    Dashboard\n  },\n  data: () => {\n    return {\n      httpbody: '',\n      fileData: ''\n    };\n  },\n  computed: {\n    uppy: () => new Uppy().use(XHRUpload, {\n      endpoint: \"http://10.1.30.202:5000/upload\",\n      fieldName: \"file\",\n      bundle: true,\n      autoProceed: false\n    }).on('upload-success', (file, response) => {\n      console.log(file.name, response);\n      const httpbody = response.body;\n      this.fileData = JSON.stringify(httpbody);\n    })\n  },\n  beforeDestroy() {\n    this.uppy.close({\n      reason: 'unmount'\n    });\n  }\n};","map":{"version":3,"mappings":"AASA,SAASA,SAAQ,QAAS,WAAW;AACrC,OAAO,2BAA2B;AAClC,OAAOC,IAAG,MAAO,YAAY;AAC7B,OAAOC,SAAQ,MAAO,kBAAkB;AACxC,OAAO,2BAA2B;AAClC,OAAO,gCAAgC;AAEvC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAI,EAAE,MAAM;IACV,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,IAAI,EAAE,MAAM,IAAIR,IAAI,EAAC,CACpBS,GAAG,CAACR,SAAS,EAAE;MACdS,QAAQ,EAAE,gCAAgC;MAC1CC,SAAS,EAAE,MAAM;MACjBC,MAAM,EAAE,IAAI;MACZC,WAAW,EAAE;IACb,CAAC,EAEFC,EAAE,CAAC,gBAAgB,EAAE,CAACC,IAAI,EAAEC,QAAQ,KAAK;MAC1CC,OAAO,CAACC,GAAG,CAACH,IAAI,CAACb,IAAI,EAAEc,QAAQ;MAC/B,MAAMX,QAAO,GAAIW,QAAQ,CAACG,IAAG;MAC7B,IAAI,CAACb,QAAO,GAAIc,IAAI,CAACC,SAAS,CAAChB,QAAQ;IAC3C,CAAC;EACC,CAAC;EACDiB,aAAY,GAAK;IACf,IAAI,CAACd,IAAI,CAACe,KAAK,CAAC;MAAEC,MAAM,EAAE;IAAU,CAAC;EACvC;AACF","names":["Dashboard","Uppy","XHRUpload","name","components","data","httpbody","fileData","computed","uppy","use","endpoint","fieldName","bundle","autoProceed","on","file","response","console","log","body","JSON","stringify","beforeDestroy","close","reason"],"sourceRoot":"","sources":["/lab/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <dashboard\n      ref=\"dash\"\n      :uppy=\"uppy\"\n    />\n  </div>\n</template>\n<script>\nimport { Dashboard } from \"@uppy/vue\";\nimport \"@uppy/core/dist/style.css\";\nimport Uppy from \"@uppy/core\";\nimport XHRUpload from \"@uppy/xhr-upload\";\nimport \"@uppy/core/dist/style.css\";\nimport \"@uppy/dashboard/dist/style.css\";\n\nexport default {\n  name: 'App',\n  components: {\n    Dashboard\n  },\n  data: () => {\n    return {\n      httpbody: '',\n      fileData: '',\n    };\n  },\n  computed: {\n    uppy: () => new Uppy()\n    .use(XHRUpload, {\n      endpoint: \"http://10.1.30.202:5000/upload\",\n      fieldName: \"file\",\n      bundle: true,\n      autoProceed: false,\n      })\n\n    .on('upload-success', (file, response) => {\n    console.log(file.name, response)\n    const httpbody = response.body\n    this.fileData = JSON.stringify(httpbody)\n})\n  },\n  beforeDestroy () {\n    this.uppy.close({ reason: 'unmount' })\n  }\n}\n</script>"]},"metadata":{},"sourceType":"module"}